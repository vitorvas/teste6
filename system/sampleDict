FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;
  
  setFormat       raw;
  
  sets
  (
      coolant
      {
          type    uniform;
          axis    y;
          start   ( 0 0 0.36 );
          end     ( 0 0.5 0.36 );
          nPoints 10;
      }
  );
  
  fields          (T); 
