FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     thesisCoupledFoam;

startFrom       latestTime; //startTime;

startTime       0;

stopAt          endTime;

endTime         800; //50.0;

deltaT          1; //1.0;

writeControl    timeStep;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


// InfoSwithes
// {
//   allowSystemOperations 1;
// }

