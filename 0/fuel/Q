FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fuel";
    object      Q;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -3 0 0 0 0 ];

internalField   uniform 1.13815e+07;

boundaryField
{
    parede
    {
        type            calculated;
    }
    extremos
    {
        type            calculated;
    }
    inlet
    {
        type            calculated;
    }
    outlet
    {
        type            calculated;
    }
    fuel_to_cladding
    {
        type            calculated;
    }
}


