FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/cladding";
    object      wallHeatFlux;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    extremos
    {
        type            calculated;
        value           uniform 0;
    }
    cladding_to_fuel
    {
        type            calculated;
        value           uniform 0;
    }
    cladding_to_coolant
    {
        type            calculated;
        value           uniform 0;
    }
}


