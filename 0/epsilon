FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    parede
    {
	type            symmetry;
        // type            wall;
        // value           uniform 0;
    }
    extremos
    {
        type            wall;
	value		uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    coolant_to_cladding
    {
        type            calculated;
        value           uniform 0;
    }
    fc
    {
	type zeroGradient;
    }
    cc
    {
	type zeroGradient;
    }
}


